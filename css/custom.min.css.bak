.solution-explore-secondary-posts {display: flex;}
.solution-explore-secondary-posts a {width: 100%; background-color: var(--yellow); padding: 15px 5px; text-align: center; box-sizing: border-box; display: flex; align-items: center; justify-content: center; font-size: 14px; font-weight: 600; line-height: 1.4; font-family: var(--font-primary); color: var(--dark); transition:all ease-in-out .3s; letter-spacing: 1px; text-transform: uppercase;}
.solution-explore-secondary-posts a:hover {background-color: var(--blue); color: var(--white);}
.solution-explore-secondary-posts a img {height: 40px; width: auto; margin-right: 10px;}
.solution-explore-secondary-posts a:not(:last-child) {border-right: 2px solid var(--white);}
.solution-explore-section-wrapper.accent-bg {position: relative;}
.solution-explore-section-wrapper.accent-bg::before {content: ""; position: absolute; top: 0; left: 0; width: 100%; height: 50%; background-color: #fffbf1;}
.featured-tag {display: inline-block; position: absolute; top: 25px; right: 25px; color: var(--white); font-size: 16px; letter-spacing: 1px; box-sizing: border-box; padding: 8px 10px; background: var(--blue);}
.solution-explore-container h2 {color: var(--white);}
.solution-explore-primary-text h3 {color: var(--yellow); font-size: 35px;}
.solution-explore-primary-text .readon.yellow.solid:hover {background-color: var(--blue); color: var(--white); border-color: var(--blue);}

.carousel-testimonials {position: relative;}
.carousel-testimonials.accent-light-blue::before {content: ""; position: absolute; top: -25%; left: 0; width: 100%; height: 100%; background: #f8f7f7; z-index: -1;}
