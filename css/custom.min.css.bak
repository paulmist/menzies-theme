body p {font-family: "roboto", sans-serif!important;}
h3.author__name {font-size: 40px!important;}
.readon {text-decoration: none;}

/* SEARCH FEATURE */
#menu-item-200 > a {background-color: var(--dark); box-sizing: border-box; padding: 5px; height: 50px; width: 50px; color: var(--white); letter-spacing: 1px; transition: all 0.3s ease-in-out; position: relative; z-index: 0; font-size: 13px; line-height: 16px; text-align: center; overflow: hidden; font-weight: 600; text-transform: uppercase; border: 3px solid var(--dark); display: flex; align-items: center; justify-content: center; text-align: center; transition: all 0.3s ease-in-out;}
#menu-item-200 > a:hover {background-color: var(--yellow); color: var(--dark); border-color: var(--yellow);}
.menzies-nav .readon {margin-bottom: 0;}

/* MENZIES MULTI-STEP FORM TEST */
.temp-container {display: flex; justify-content: center; align-items: center; text-align: center; box-sizing: border-box; padding: 500px;}
.employees_question_tile .wpcf7-list-item label {display: flex; flex-direction: column-reverse; align-items: center;}
.employees_question_tile .wpcf7-list-item label input {height: 20px; width: 20px; cursor: pointer;}
.employees_question_tile .wpcf7-list-item-label {font-size: 25px; line-height: 35px; font-weight: 700; margin-bottom: 25px;}
.how_many_employees_container {display: flex!important; justify-content: space-between;}
.employees_question_tile .wpcf7-list-item {margin: 0;}
.employees_question_tile input[type='radio'] {appearance: none; width: 25px; height: 25px; border: 2px solid #ffc500; border-radius: 50%; background-clip: content-box; padding: 3px; cursor: pointer;}
.employees_question_tile input[type='radio']:checked {background-color: #ffc500;}
.how-many-employees-step button {background-color: #ffc500!important; color: #ffffff!important;}
.interactive-form-wrapper {padding: 250px 0;}
.page-id-14699 form.wpcf7-form {background-color: unset!important;}
.interactive-form-global-container {display: flex; justify-content: center; align-items: center; text-align: center;}
.interactive-form-global-container li:not(.tab-nav-link) {list-style: none; position: relative; margin: 15px 0;}
.interactive-form-global-container ul {margin: 0;}
.interactive-form-global-container button {background-color: #ffc500; color: #ffffff!important; cursor: pointer; font-size: 18px; line-height: 1.4; box-sizing: border-box; display: inline-flex; align-items: center; padding: 15px 20px;letter-spacing: 1px; font-weight: 400; transition: all 0.3s ease-in-out; position: relative; z-index: 0; text-align: center; margin: 0; overflow: hidden; border-radius: 0px; border: none;}
.interactive-list-special li::before {content: ""; position: absolute; height: 4px; width: 20px; display: inline-block; border-radius: 50px; transition: all .3s ease-in-out; background: #ffc500; border-style: unset;}
.interactive-form-global-container p:nth-child(6) {display: flex!important;justify-content: center!important;}
.interactive-title-container {padding-left: 25px; padding-right: 25px;}
.interactive-form-global-container .wpcf7-spinner {display: none;}
.interactive-title-container h3 {font-size: 35px!important;line-height: 1.2!important;}
.interactive-form-global-container h4 {margin-bottom: 20px;}

/* INTERACTIVE FORM TILES */
.interactive_form_container {display: flex; justify-content: space-between; box-sizing: border-box; padding: 0;}
.employees_question_tile {width: calc(33.333% - 15px); box-sizing: border-box; padding: 40px 50px; background-color: #fffefc; border: solid 2px #ffc500; box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px; display: flex; flex-direction: column; justify-content: center; align-items: center;}
.employees_question_tile .wpcf7-list-item label {display: flex; flex-direction: column-reverse; align-items: center;}
.employees_question_tile .wpcf7-list-item label input {height: 20px; width: 20px; cursor: pointer;}
.employees_question_tile .wpcf7-list-item-label {font-size: 25px; line-height: 35px; font-weight: 700; margin-bottom: 25px;}
.employees_question_tile.hr_tile {width: calc(50% - 15px);}
.employees_question_tile .wpcf7-list-item {margin: 0;}
.interactive_form_container .employees_question_tile input[type='radio'] {appearance: none; width: 25px; height: 25px!important; border: 2px solid #ffc500!important; border-radius: 50%!important; background-clip: content-box; padding: 3px!important; cursor: pointer;}
.interactive_form_container .employees_question_tile input[type='radio']:checked {background-color: #ffc500;}
.employees_question_tile > h3 {font-size: 25px!important; line-height: 35px!important; font-family: var(--font-primary); margin: 20px 0;}
.employees_question_tile img {height: 100%; width: 100%; max-width: 60px; object-fit: contain;}
.employees_question_tile p:nth-child(2) {display: none;}
.interactive_form_container.hr_container {justify-content: space-between;}
.uacf7-step, .uacf7_conditional {width: 100%; max-width: 1200px; margin: 0 auto;}
.interactive-form-global-container .interactive_form_container.contact-step-two p:nth-child(5) {display: flex; justify-content: center;}
